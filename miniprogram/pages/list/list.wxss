/* miniprogram/pages/list/list.wxss */
.container {
  display: flex;
  box-sizing: border-box;
  flex-wrap: wrap;
  padding: 2%;
  background: #ebebeb;
  justify-content: space-between;
}

.imgBox {
  width: 100%;
  height: 444rpx;
  overflow: hidden;
}

.starItem {
  width: 48%;
  /* overflow: hidden; */
  background: #fff;
  margin-bottom: 10rpx;
}

.imgItem {
  width: 100%;
  height: 100%;
}

.introduce {
  margin: 10rpx 15rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  height: 84rpx;
}
.userImg {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  margin: 20rpx;
}
.userInfo {
  display: flex;
  align-items: center;
}
